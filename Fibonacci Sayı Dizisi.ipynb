   "source": [
    "def fibonacci(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    else:\n",
    "        return fibonacci(n-1) + fibonacci(n-2)\n",
    " \n",
    "sayi = int(input(\"Kaç terimli Fibonacci dizisi oluşturmak istiyorsunuz? \"))\n",
    " \n",
    "for i in range(sayi):\n",
    "    print(fibonacci(i), end=\", \")"
   ]
  }
